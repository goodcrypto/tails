msgid "[[!meta title=\"Numerous security holes in Tails 2.0.1\"]]\n"
msgstr "[[!meta title=\"Zahlreiche Sicherheitslücken in Tails 2.0.1\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!tag security/fixed]]\n"
